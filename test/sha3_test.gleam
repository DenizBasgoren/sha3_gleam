}

fn to_state(state: String) -> State {
  state
  |> to_listbits
  |> state.new
}

fn to_inputblock(str: String, flavor: types.Sha3Flavor) -> InputBlock {
  str
  |> to_listbits
  |> input_block.new(flavor)
}

